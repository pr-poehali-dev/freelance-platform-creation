{
  "tests": [
    {
      "name": "OPTIONS request for CORS",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200,
      "expectedHeaders": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "name": "Delete order - no auth",
      "method": "DELETE",
      "path": "/?order_id=1",
      "expectedStatus": 401,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Delete order - missing order_id",
      "method": "DELETE",
      "path": "/",
      "headers": {
        "X-User-Id": "123"
      },
      "expectedStatus": 400,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    }
  ]
}
